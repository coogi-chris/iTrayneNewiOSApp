# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

def global_pods
 
  use_frameworks!

  # Pods for iTrayne_Trainer
  pod 'SwiftLocation', '~> 4.0'
  pod 'CurrencyText'  
  pod 'PhoneNumberKit', '~> 3.1'
  pod 'SwiftDate'
  pod 'Firebase'
  pod 'Firebase/Storage'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'FirebaseFirestoreSwift'
  pod 'URLImage'
  pod 'Firebase/Functions'
  pod 'Stripe'
end

target 'iTrayne Client' do
  global_pods
end

target 'iTrayne Trainer' do
  global_pods
end
